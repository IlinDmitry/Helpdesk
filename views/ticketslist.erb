<h1 style="margin-top: 20px; margin-bottom: 20px;">Service Request</h1>

<table class="table table-striped">
  <thead class="thead-inverse">
  <tr>
    <th>Request ID</th>
    <th>Complaint</th>
    <th>Last Updated</th>
  </tr>
  </thead>
  <tbody>
  <% @list.each do |iter| %>
      <tr>
        <td><%= iter[:code] %></td>
        <td>
          <span class="badge badge-info"><%= iter[:status] %></span>
          <%= iter[:complaint] %>
        </td>
        <td><%= iter[:updatedat] %></td>
      </tr>
  <% end%>
